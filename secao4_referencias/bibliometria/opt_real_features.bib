@article{ WOS:000459297900002,
Author = {Baruffaldi, Giulia and Accorsi, Riccardo and Manzini, Riccardo},
Title = {Warehouse management system customization and information availability
   in 3pl companies A decision-support tool},
Journal = {INDUSTRIAL MANAGEMENT \& DATA SYSTEMS},

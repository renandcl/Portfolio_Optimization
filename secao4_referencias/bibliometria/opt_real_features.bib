@article{ WOS:000508850100003,
Author = {Taljaard, B. H. and Mare, E.},
Title = {Considering the use of an equal-weighted index as a benchmark for South
   African equity investors},
Journal = {SOUTH AFRICAN ACTUARIAL JOURNAL},
Year = {2019},
Volume = {19},
Pages = {52-69},
Abstract = {We analyse and discuss the use of an equal-weighted index as an
   alternative to the market capitalisation weighted (cap-weighted) index
   as a benchmark for active equity portfolios in the South African equity
   market. Our findings indicate that equal-weighted portfolios are, in
   general, more efficient than cap-weighted portfolios and that random
   active portfolios tend to display significantly improved risk-return
   characteristics when using an equal-weighted index as a benchmark. We
   find our results are robust to transaction costs involved with
   rebalancing.},

@article{ WOS:000907272300001,
Author = {Razak, Muhammad Zaim},
Title = {The dynamic role of the Japanese property sector REITs in mixed-assets
   portfolio},
Journal = {JOURNAL OF PROPERTY INVESTMENT \& FINANCE},

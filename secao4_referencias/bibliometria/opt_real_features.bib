@article{ WOS:000634568400013,
Author = {Paolella, Marc S. and Polak, Pawel and Walker, Patrick S.},
Title = {A non-elliptical orthogonal GARCH model for portfolio selection under
   transaction costs},
Journal = {JOURNAL OF BANKING \& FINANCE},
Year = {2021},
Volume = {125},
Month = {APR},
Abstract = {Covariance matrix forecasts for portfolio optimization have to balance
   sensitivity to new data points with stability in order to avoid
   excessive rebalancing. To achieve this, a new orthogonal GARCH model for
   a multivariate set of non-Gaussian asset returns is proposed. The
   conditional return distribution is multivariate generalized hyperbolic
   and the dispersion matrix dynamics are driven by the leading factors in
   a principal component decomposition. Each of these leading factors is
   endowed with a univariate GARCH structure, while the remaining
   eigenvalues are kept constant over time. Joint maximum likelihood
   estimation of all model parameters is performed via an expectation
   maximization algorithm, and is applicable in high dimensions. The new
   model generates realistic correlation forecasts even for large asset
   universes and captures rising pairwise correlations in periods of market
   distress better than numerous competing models. When applied to
   portfolio optimization, it generates strategies with lower turnover and
   maximum drawdown, and superior risk-adjusted returns net of transaction
   costs. Moreover, unlike its competitors, it performs well in the sudden
   market downturn triggered by the global COVID-19 pandemic.

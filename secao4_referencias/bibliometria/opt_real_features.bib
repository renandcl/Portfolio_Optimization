@article{ WOS:000681749800009,
Author = {Wu, Lan and Wu, Shuo},
Title = {How Fast Does It Diverge? Discrete Hedging Error with Transaction Costs},
Journal = {ACTA MATHEMATICAE APPLICATAE SINICA-ENGLISH SERIES},
Year = {2021},
Volume = {37},

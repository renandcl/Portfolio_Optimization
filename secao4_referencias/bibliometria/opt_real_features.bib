@article{ WOS:000666761500001,
Author = {Nakagawa, Kei and Ito, Katsuya},
Title = {Taming Tail Risk: Regularized Multiple beta Worst-Case CVaR Portfolio},
Journal = {SYMMETRY-BASEL},
Year = {2021},

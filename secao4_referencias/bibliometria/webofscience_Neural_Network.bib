Title = {Machine Trading by Time Series Models and Portfolio Optimization},
Booktitle = {PROCEEDINGS OF THE 2019 4TH INTERNATIONAL CONFERENCE ON INFORMATION
   TECHNOLOGY (INCIT): ENCOMPASSING INTELLIGENT TECHNOLOGY AND INNOVATION
   TOWARDS THE NEW ERA OF HUMAN LIFE},

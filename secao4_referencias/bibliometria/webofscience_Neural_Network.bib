@inproceedings{ WOS:000704288404034,
Author = {Pauwels, Ruben and Tsiligianni, Evaggelia and Deligiannis, Nikos},
Book-Group-Author = {IEEE},
Title = {HCGM-NET: A DEEP UNFOLDING NETWORK FOR FINANCIAL INDEX TRACKING},
Booktitle = {2021 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP 2021)},
Year = {2021},
Pages = {3910-3914},
Note = {IEEE International Conference on Acoustics, Speech and Signal Processing
   (ICASSP), ELECTR NETWORK, JUN 06-11, 2021},
Organization = {IEEE; Inst Elect \& Elect Engineers, Signal Proc Soc},
Abstract = {Tracking the performance of a financial index by selecting a subset of
   assets composing the index is a problem that raises several difficulties
   due to the large size of the stock market. Typically, optimisation
   algorithms with high complexity are employed to address such problems.
   In this paper, we focus on sparse index tracking and employ a
   Frank-Wolfe-based algorithm which we translate into a deep neural
   network, a strategy known as deep unfolding. Numerical experiments show
   that the learned model outperforms the iterative algorithm, leading to
   high accuracy at a low computational cost. To the best of our knowledge,
   this is the first deep unfolding design proposed for financial data
   processing.},

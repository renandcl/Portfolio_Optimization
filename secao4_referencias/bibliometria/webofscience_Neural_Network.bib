@article{ WOS:000732361900001,
Author = {Baes, Michel and Herrera, Calypso and Neufeld, Ariel and Ruyssen, Pierre},
Title = {Low-Rank Plus Sparse Decomposition of Covariance Matrices Using Neural
   Network Parametrization},
Journal = {IEEE TRANSACTIONS ON NEURAL NETWORKS AND LEARNING SYSTEMS},
Year = {2021},
Month = {2021 JUL 6},
Abstract = {This article revisits the problem of decomposing a positive semidefinite
   matrix as a sum of a matrix with a given rank plus a sparse matrix. An
   immediate application can be found in portfolio optimization, when the
   matrix to be decomposed is the covariance between the different assets
   in the portfolio. Our approach consists in representing the low-rank
   part of the solution as the product MMT, where M is a rectangular matrix
   of appropriate size, parametrized by the coefficients of a deep neural
   network. We then use a gradient descent algorithm to minimize an
   appropriate loss function over the parameters of the network. We deduce
   its convergence rate to a local optimum from the Lipschitz smoothness of
   our loss function. We show that the rate of convergence grows
   polynomially in the dimensions of the input-output, and the size of each
   of the hidden layers.},

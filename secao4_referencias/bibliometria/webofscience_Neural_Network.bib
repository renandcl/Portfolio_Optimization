Title = {Estimating Return Rate of Blockchain Financial Product by ANFIS-PSO
   Method},
Booktitle = {INTELLIGENT AND FUZZY SYSTEMS: DIGITAL ACCELERATION AND THE NEW NORMAL,
   INFUS 2022, VOL 1},

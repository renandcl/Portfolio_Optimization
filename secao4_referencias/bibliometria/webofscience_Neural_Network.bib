@inproceedings{ WOS:000889132600016,
Author = {Candar, Mert and Ustundag, Alp},
Editor = {Kahraman, C and Tolga, AC and Onar, SC and Cebi, S and Oztaysi, B and Sari, IU},
Title = {Equity Portfolio Optimization Using Reinforcement Learning: Emerging
   Market Case},
Booktitle = {INTELLIGENT AND FUZZY SYSTEMS: DIGITAL ACCELERATION AND THE NEW NORMAL,
   INFUS 2022, VOL 2},

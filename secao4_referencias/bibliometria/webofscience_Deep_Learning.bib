@inproceedings{ WOS:000716986400048,
Author = {Hieu K Cao and Han K Cao and Binh T Nguyen},
Editor = {Lauw, HW and Wong, RCW and Ntoulas, A and Lim, EP and Ng, SK and Pan, SJ},
Title = {DELAFO: An Efficient Portfolio Optimization Using Deep Neural Networks},
Booktitle = {ADVANCES IN KNOWLEDGE DISCOVERY AND DATA MINING, PAKDD 2020, PT I},
Series = {Lecture Notes in Artificial Intelligence},
Year = {2020},
Volume = {12084},

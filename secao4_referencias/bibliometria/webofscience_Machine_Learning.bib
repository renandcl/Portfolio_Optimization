@article{ WOS:000473398500001,
Author = {Du, NingNing and Liu, Yan-Kui and Liu, Ying},
Title = {New Safe Approximation of Ambiguous Probabilistic Constraints for
   Financial Optimization Problem},
Journal = {DISCRETE DYNAMICS IN NATURE AND SOCIETY},
Year = {2019},
Volume = {2019},
Abstract = {In financial optimization problem, the optimal portfolios usually depend
   heavily on the distributions of uncertain return rates. When the
   distributional information about uncertain return rates is partially
   available, it is important for investors to find a robust solution for
   immunization against the distribution uncertainty. The main contribution
   of this paper is to develop an ambiguous value-at-risk (VaR)
   optimization framework for portfolio selection problems, where the
   distributions of uncertain return rates are partially available. For
   tractability consideration, we deal with new safe approximations of
   ambiguous probabilistic constraints under two types of random
   perturbation sets and obtain two equivalent tractable formulations of
   the ambiguous probabilistic constraints. Finally, to demonstrate the
   potential for solving portfolio optimization problems, we provide a
   practical example about the Chinese stock market. The advantage of the
   proposed robust optimization method is also illustrated by comparing it
   with the existing optimization approach via numerical experiments.},

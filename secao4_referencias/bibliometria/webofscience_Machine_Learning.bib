@inproceedings{ WOS:000509687902010,
Author = {Chen, Lin and Zhang, Mingrui and Karbasi, Amin},
Editor = {Chaudhuri, K and Sugiyama, M},
Title = {Projection-Free Bandit Convex Optimization},
Booktitle = {22ND INTERNATIONAL CONFERENCE ON ARTIFICIAL INTELLIGENCE AND STATISTICS,
   VOL 89},

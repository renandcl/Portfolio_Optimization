@article{ WOS:000685268100018,
Author = {Guo, Sini and Gu, Jia-Wen and Ching, Wai-Ki},
Title = {Adaptive online portfolio selection with transaction costs},
Journal = {EUROPEAN JOURNAL OF OPERATIONAL RESEARCH},

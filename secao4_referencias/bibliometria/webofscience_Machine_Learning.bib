Title = {New Features for Continuous Exploratory Landscape Analysis based on the
   SOO Tree},
Booktitle = {FOGA'19: PROCEEDINGS OF THE 15TH ACM/SIGEVO CONFERENCE ON FOUNDATIONS OF
   GENETIC ALGORITHMS},

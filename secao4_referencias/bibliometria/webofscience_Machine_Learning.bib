Title = {Orthogonal Bandit Learning for Portfolio Selection Under Cardinality
   Constraint},
Booktitle = {COMPUTATIONAL SCIENCE AND ITS APPLICATIONS - ICCSA 2019, PT III: 19TH
   INTERNATIONAL CONFERENCE, SAINT PETERSBURG, RUSSIA, JULY 1-4, 2019,
   PROCEEDINGS, PART III},

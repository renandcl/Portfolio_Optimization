@article{ WOS:000578826300036,
Author = {Lai, Zhao-Rong and Yang, Pei-Yi and Fang, Liangda and Wu, Xiaotian},
Title = {Reweighted Price Relative Tracking System for Automatic Portfolio
   Optimization},
Journal = {IEEE TRANSACTIONS ON SYSTEMS MAN CYBERNETICS-SYSTEMS},
Year = {2020},
Volume = {50},
Number = {11},
Pages = {4349-4361},
Month = {NOV},
Abstract = {In this paper, we propose a novel reweighted price relative tracking
   (RPRT) system for automatic portfolio optimization (APO). In the price
   prediction stage, it automatically assigns separate weights to the price
   relative predictions according to each asset's performance, and these
   weights will also be automatically updated. In the portfolio optimizing
   stage, a novel tracking system with a generalized increasing factor is
   proposed to maximize the future wealth of next period. Besides, an
   efficient algorithm is designed to solve the portfolio optimization
   objective, which is applicable to large-scale and time-limited
   situations. Extensive experiments on six benchmark datasets from real
   financial markets with diverse assets and different time spans are
   conducted. RPRT outperforms other state-of-the-art systems in cumulative
   wealth, mean excess return, annual percentage yield, and some typical
   risk metrics. Moreover, it can withstand considerable transaction costs
   and runs fast. It indicates that RPRT is an effective and efficient APO
   system.},

@article{ WOS:000556704300001,
Author = {Lim, Sangmin and Kim, Man-Je and Ahn, Chang Wook},
Title = {A Genetic Algorithm (GA) Approach to the Portfolio Design Based on
   Market Movements and Asset Valuations},

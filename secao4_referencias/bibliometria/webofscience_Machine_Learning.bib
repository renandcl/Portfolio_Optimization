@article{ WOS:000691377800022,
Author = {Wu, Zeyang and Nip, Kameng and He, Qie},
Title = {A New Combinatorial Algorithm for Separable Convex Resource Allocation
   with Nested Bound Constraints},
Journal = {INFORMS JOURNAL ON COMPUTING},

@inproceedings{ WOS:000841686400042,
Author = {Ivonne Lozano-Medina, Jessica and Hervert-Escobar, Laura and
   Hernandez-Gress, Neil},
Editor = {Krzhizhanovskaya, VV and Zavodszky, G and Lees, MH and Dongarra, JJ and Sloot, PMA and Brissos, S and Teixeira, J},
Title = {Risk Profiles of Financial Service Portfolio for Women Segment Using
   Machine Learning Algorithms},
Booktitle = {COMPUTATIONAL SCIENCE - ICCS 2020, PT VII},
Series = {Lecture Notes in Computer Science},
Year = {2020},
Volume = {12143},
Pages = {561-574},
Note = {20th Annual International Conference on Computational Science (ICCS),
   Amsterdam, NETHERLANDS, JUN 03-05, 2020},
Organization = {Univ Amsterdam; NTU Singapore; Univ Tennessee},
Abstract = {Typically, women are scored with a lower financial risk than men.
   However, the understanding of variables and indicators that lead to such
   results, are not fully understood. Furthermore, the stochastic nature of
   the data makes it difficult to generate a suitable profile to offer an

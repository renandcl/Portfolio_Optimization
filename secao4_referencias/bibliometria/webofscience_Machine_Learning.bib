@article{ WOS:000838510500001,
Author = {Yulianti, Lenny Putri and Surendro, Kridanto},
Title = {Implementation of Quantum Annealing: A Systematic Review},
Journal = {IEEE ACCESS},
Year = {2022},
Volume = {10},
Pages = {73156-73177},
Abstract = {Quantum annealing is a quantum computing approach widely used for
   optimization and probabilistic sampling problems. It is an alternative
   approach designed due to the limitations of gate-based quantum computing
   models. The method is observed to have a significant impact on different
   fields such as machine learning, graphics, routing, scheduling,
   computational chemistry, computational biology, security, portfolio, and
   others despite the fact that it is relatively new. This research
   provides a systematic review of research development trends in the field
   of quantum annealing and analyzes how it has been implemented in
   different problem domains. The results are expected to serve as the
   basis to identify the opportunities and challenges of research related
   to its implementation. The main contribution of this systematic review
   is to summarize different implementations of quantum annealing. It is
   also to analyze the prospect and opportunities in one of the problem
   domains with the greatest interest which is machine learning.},

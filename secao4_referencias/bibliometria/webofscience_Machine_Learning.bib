@inproceedings{ WOS:000605292300098,
Author = {Jankovic, Anja and Doerr, Carola},
Book-Group-Author = {Assoc Comp Machinery},
Title = {Landscape-Aware Fixed-Budget Performance Regression and Algorithm
   Selection for Modular CMA-ES Variants},
Booktitle = {GECCO'20: PROCEEDINGS OF THE 2020 GENETIC AND EVOLUTIONARY COMPUTATION
   CONFERENCE},

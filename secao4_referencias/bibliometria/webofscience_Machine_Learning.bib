@article{ WOS:000836533800001,
Author = {Rubesam, Alexandre},
Title = {Machine learning portfolios with equal risk contributions: Evidence from
   the Brazilian market},
Journal = {EMERGING MARKETS REVIEW},
Year = {2022},
Volume = {51},
Number = {B},
Month = {JUN},
Abstract = {We investigate the use of machine learning (ML) to forecast stock
   returns in the Brazilian market using a rich proprietary dataset. While
   ML portfolios can easily outperform the local market, the performance of
   long-short strategies using ML is hampered by the high volatility of the
   short portfolios. We show that an Equal Risk Contribution (ERC) approach
   significantly improves risk-adjusted returns. We further develop an ERC
   approach that combines multiple long-short strategies obtained with ML
   models, equalizing risk contributions across ML models, which
   outperforms, on a risk-adjusted basis, all individual ML long-short
   strategies, as well as alternative combinations of ML strategies.},
